@page "/AboutUs"
@using Microsoft.EntityFrameworkCore
@using Microsoft.AspNetCore.Localization
@using LEotA.Services
@model LEotA.Pages.AboutUsModel
@{
    ViewBag.Title = "My Home Page";
    var culture = HttpContext.Features.Get<IRequestCultureFeature>().RequestCulture.Culture.Name;
}
<div class="container border border-2">
    <h2>About Us</h2>
    @{
        try
        {
            foreach (var aboutUs in ((List<AboutUs>) ViewData["aboutUs"]))
            {
                <tr>
                    @* <img class="float-right" src="data:image/jpeg;base64,@Convert.ToBase64String(aboutUs.Image)"/> *@
                    <p class="align-center"> @LocalizationHelper.GetTextByCulture(aboutUs.Text, culture) </p>

                    @* <img alt="" src="@ViewData["img8642ECA1-1577-40C8-6FAA-08D98E3B2BA1"]" style="height:256px;width:256px;"/> *@

                    @* <td>@product.Id</td> *@
                    @* <td>@product.Text</td> *@
                    @* <td>@product.Image</td> *@
                </tr>
            }
        }
        catch (Exception e)
        {
            <p>NULL</p>
        }
    }
</div>
